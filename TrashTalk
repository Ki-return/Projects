localPlayer = game.Players.LocalPlayer
localPlayer:GetMouse().KeyDown:Connect(function(Key)
    if Key == getgenv().Keybind then
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(Text1, "All")
        wait()
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(Text2, "All")
        wait()
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(Text3, "All")
        wait()
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(Text4, "All")
        wait()
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(Text5, "All")
        wait()
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(Text6, "All")
        wait()
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(Text7, "All")
        wait()
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(Text8, "All")
        wait()
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(Text9, "All")
        wait()
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(Text10, "All")
    end
end)
